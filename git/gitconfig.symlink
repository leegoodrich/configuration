[user]
    email = goodrich@amazon.com
    name = Lee Goodrich
[push]
    default = simple
[alias]
    st  = status
    ci  = commit
    br  = branch
    co  = checkout
    df  = diff
    dc  = diff --cached
    lg  = log -p
    who = shortlog -s --
    wtf = !$CONFIG_ROOT/bin/git/git-wtf
    promote = !$CONFIG_ROOT/bin/git/git-promote
[core]
        excludesfile = ~/.gitignore
        editor = vim
